# .env.example
# Copy this file to .env and adjust values

# Environment
ENV=development
DEBUG=true

# Database
DB_URL=postgresql://postgres:password123@localhost:5432/dexbrain
DB_POOL_SIZE=5
DB_MAX_OVERFLOW=10

# Redis
REDIS_URL=redis://localhost:6379/0
REDIS_POOL_SIZE=5
REDIS_DEFAULT_TTL=3600

# Solana
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com
SOLANA_COMMITMENT=confirmed
SOLANA_TIMEOUT=30

# API
API_HOST=0.0.0.0
API_PORT=8000
API_WORKERS=4
API_DEBUG=true
API_CORS_ORIGINS=["*"]

# Logging
LOG_LEVEL=INFO
LOG_DIRECTORY=logs

# Meteora
METEORA_UPDATE_INTERVAL=60