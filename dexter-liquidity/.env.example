# =================================================================
# DEXTER LIQUIDITY MANAGEMENT ENVIRONMENT CONFIGURATION
# =================================================================
# Copy this file to .env and fill in your actual values

# =================================================================
# API KEYS (Required)
# =================================================================
ALCHEMY_API_KEY=your_alchemy_api_key_here
BASESCAN_API_KEY=your_basescan_api_key_here

# =================================================================
# BLOCKCHAIN CONFIGURATION
# =================================================================
BASE_RPC_URL=https://base-mainnet.g.alchemy.com/v2/YOUR_API_KEY
UNISWAP_SUBGRAPH_URL=https://api.thegraph.com/subgraphs/name/messari/uniswap-v3-base

# =================================================================
# DATABASE CONFIGURATION
# =================================================================
DATABASE_URL=postgresql://user:password@localhost:5432/dexter
REDIS_URL=redis://:your_redis_password@localhost:6379

# =================================================================
# AGENT CONFIGURATION
# =================================================================
ENVIRONMENT=development
PARALLEL_WORKERS=4
MAX_SLIPPAGE=0.005

# =================================================================
# PERFORMANCE MONITORING
# =================================================================
LOG_LEVEL=INFO
PROMETHEUS_PORT=9091
ENABLE_METRICS=true

# =================================================================
# ML/AI INTEGRATION
# =================================================================
DEXBRAIN_API_URL=http://localhost:8080
ML_ENABLED=true
LEARNING_MODE=passive

# =================================================================
# UNISWAP V4 CONFIGURATION (Optional)
# =================================================================
V4_HOOKS_ENABLED=false
V4_FACTORY_ADDRESS=your_v4_factory_address_here
V4_POOL_MANAGER_ADDRESS=your_v4_pool_manager_address_here